<manifest>
    <define>
        <domain name="bt_agent"/>
	<request>
		<smack request="aul::launch" type="x"/>
		<smack request="device::sys_logging" type="w"/>
		<smack request="system::share" type="rwxat"/>
		<smack request="sys-assert::core" type="rwxat"/>
		<smack request="dbus" type="rwx"/>
		<smack request="msg-service" type="rwx"/>
		<smack request="msg-service::db" type="rw"/>
		<smack request="msg-service::read" type="rw"/>
		<smack request="msg-service::write" type="rw"/>
		<smack request="msg-service::smstrigger" type="rw"/>
		<smack request="telephony_framework::api_sim" type="r"/>
		<smack request="telephony_framework::api_sms" type="r"/>
		<smack request="telephony_framework::properties" type="rw"/>
		<smack request="contacts-service::svc" type="rw"/>
		<smack request="contacts-service::phonelog" type="rw"/>
		<smack request="device::app_logging" type="w"/>
		<smack request="deviced::display" type="rw"/>
		<smack request="bt-service::public" type="w"/>
		<smack request="bt-service::platform" type="rw"/>
		<smack request="tizen::vconf::public::r" type="rw"/>
		<smack request="tizen::vconf::platform::rw" type="rw"/>
		<smack request="tizen::vconf::public::r::platform::rw" type="rw"/>
		<smack request="tizen::vconf::setting::admin" type="rw"/>
		<smack request="tizen::vconf::telephony::admin" type="rw"/>
	</request>
	<permit>
		<smack permit="dbus" type="rwx"/>
	</permit>
    </define>
    <assign>
        <filesystem path="/usr/share/dbus-1/services/org.bluez.map_agent.service" label="_"/>
        <filesystem path="/usr/share/dbus-1/services/org.bluez.pb_agent.service" label="_"/>
        <dbus name="org.bluez.ag_agent" own="bluetooth-ag-agent" bus="system">
            <node name="/org/bluez/hfp_agent">
                <interface name="Org.Hfp.App.Interface">
                    <method name="Play">
                        <annotation name="com.tizen.smack" value="bt-service::platform" />
                    </method>
                    <method name="Stop">
                        <annotation name="com.tizen.smack" value="bt-service::platform" />
                    </method>
                    <method name="ChangeCallStatus">
                        <annotation name="com.tizen.smack" value="bt-service::platform" />
                    </method>
                    <method name="SetSpeakerGain">
                        <annotation name="com.tizen.smack" value="bt-service::platform" />
                    </method>
                    <method name="SendVendorAtCmd">
                        <annotation name="com.tizen.smack" value="bt-service::platform" />
                    </method>
                    <method name="SetVoiceDial">
                        <annotation name="com.tizen.smack" value="bt-service::platform" />
                    </method>
                    <method name="CheckPrivilege">
                        <annotation name="com.tizen.smack" value="bt-service::platform" />
                    </method>
                </interface>
            </node>
        </dbus>
    </assign>
    <request>
        <domain name="bt_agent"/>
    </request>
</manifest>
